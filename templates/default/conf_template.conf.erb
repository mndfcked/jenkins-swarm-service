<% end %>

<% if !@name.nil? && !@name.empty? %>
<%= "wrapper.app.parameter.15 = -name" %>
<%= "wrapper.app.parameter.16 = #{@name}" %>
<% end %>

<% if !@password.nil? && !@password.empty? %>
<%= "wrapper.app.parameter.17 = -password" %>
<%= "wrapper.app.parameter.18 = #{@password}" %>
<% end %>

<% if !@username.nil? && !@username.empty? %>
<%= "wrapper.app.parameter.19 = -username" %>
