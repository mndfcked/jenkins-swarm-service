<%= render "orig_wrapper.conf.erb" %>
<% if !@auto_discovery_addressnode.nil? && !@auto_discovery_addressnode.empty? %>
<%= "wrapper.app.parameter.1 = -autoDiscoveryAddress" %>
<%= "wrapper.app.parameter.2 = #{@auto_discovery_addressnode}" %>
<% end %>

<% if !@description.nil? && !@description.empty? %>
<%= "wrapper.app.parameter.3 = -description" %>
<%= "wrapper.app.parameter.4 = #{@description}" %>
<% end %>

<% if !@disable_ssl_verification.nil? && !@disable_ssl_verification.empty? %>
<%= "wrapper.app.parameter.5 = -disableSslVerification" %>
<%= "wrapper.app.parameter.6 = #{@disable_ssl_verification}" %>
<% end %>

<% if !@executors.nil? %>
<%= "wrapper.app.parameter.7 = -executors" %>
